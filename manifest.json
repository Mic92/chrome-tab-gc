{
  "name": "Tab Garbage Collector",
  "version": "1.0",
  "manifest_version": 2,
  "icons": { "16": "icons/icon_016.png",
             "48": "icons/icon_048.png",
            "128": "icons/icon_128.png" },
  "description": "Tab Garbage Collector extension closes tabs that have not been viewed for long time.",
  "background": {
    "scripts": ["bg.js"]
  },
  "options_page": "options.html",
  "permissions": [
      "tabs"
  ]
}